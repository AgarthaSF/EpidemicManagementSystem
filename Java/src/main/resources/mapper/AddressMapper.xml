<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.agarthasf.epipre.mapper.AddressMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.agarthasf.epipre.pojo.Address">
        <id column="address_id" property="addressId" />
        <result column="address_name" property="addressName" />
        <result column="longitude" property="longitude" />
        <result column="latitude" property="latitude" />
        <result column="address_position" property="addressPosition" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        address_id, address_name, longitude, latitude, address_position
    </sql>

</mapper>
